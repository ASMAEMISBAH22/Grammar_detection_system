  # simple DP implementation\n","    if a == b:\n","        return 0\n","    la, lb = len(a), len(b)\n","    if la == 0:\n","        return lb\n","    if lb == 0:\n","        return la\n","    prev = list(range(lb + 1))\n","    for i, ca in enumerate(a, start=1):\n","        cur = [i] + [0] * lb\n","        for j, cb in enumerate(b, start=1):\n","            ins = cur[j-1] + 1\n","            delete = prev[j] + 1\n","            sub = prev[j-1] + (0 if ca == cb else 1)\n","            cur[j] = min(ins, delete, sub)\n","        prev = cur\n","    return prev[lb]\n"],"metadata":{"id":"ZzFYj_2Zc1Nv","executionInfo":{"status":"ok","timestamp":1765384989605,"user_tz":-60,"elapsed":248,"user":{"displayName":"Asmae Misbah","userId":"03840885929360177687"}}},"execution_count":8,"outputs":[]},{"cell_type":"code","source":["def detect_columns(df):\n","    # common names for source (to correct) and target (reference)\n","    src_candidates = [\"input\",\"source\",\"src\",\"sentence\",\"text\",\"orig\",\"candidate\"]\n","    ref_candidates = [\"target\",\"ref\",\"reference\",\"label\",\"correct\",\"gold\",\"corrected\"]\n","\n","    cols = [c.lower() for c in df.columns]\n","    src_col = None\n","    ref_col = None\n","\n","    for cand in src_candidates:\n","        if cand in cols:\n","            src_col = df.columns[cols.index(cand)]\n","            break\n","    for cand in ref_candidates:\n","        if cand in cols:\n","            ref_col = df.columns[cols.index(cand)]\n","            break\n","\n","    # fallback: if only one text-like column, treat it as src and no refs\n","    if src_col is None:\n","        # choose first column with dtype object\n","        for c in df.columns:\n","            if df[c].dtype == object:\n","                src_col = c\n","                break\n","    if ref_col is None:\n","        # try to pick the next object column differing from src_col\n","        for c in df.columns:\n","            if df[c].dtype == object and c != src_col:\n","                ref_col = c\n","                break\n","\n","    return src_col, ref_col\n"],"metadata":{"id":"KzWItFnXc5K7","executionInfo":{"status":"ok","timestamp":1765384993815,"user_tz":-60,"elapsed":490,"user":{"displayName":"Asmae Misbah","userId":"03840885929360177687"}}},"execution_count":9,"outputs":[]},{"cell_type":"code","source":["model_name = \"gotutiyan/gec-t5-large-clang8\"\n","device = \"cuda\" if torch.cuda.is_available() else \"cpu\"\n","\n","print(f\"Loading {model_name} on {device} ...\")\n","tokenizer = AutoTokenizer.from_pretrained(model_name)\n","model = AutoModelForSeq2SeqLM.from_pretrained(model_name)\n","model.to(device)\n","model.eval()\n","print(\"Model loaded.\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":400,"referenced_widgets":["441d129dafef4385ba03d712a6b691ea","fa1646cd294d478dadd94d94a9ed9030","4fbe22e433cb41b4b6daf7b9ace4f58e","02be10cc9ffc4728892c5b3ef978ff59","3d4747afe711436eb70b99d26acafecc","5e077cd05c7a42daa424d13a6478d7db","6b9dab30992f40c5b97b0a68ec84b407","e84e7429b609438d994a9de0233eba2b","0e11ab45b93145439586912c7e2f248f","6b0fc83ae0d4473e8f55aad59bfa421a","11f1fe6f048d4aac8849bd2b2ad91e3f","d6d1baefd4d54594843abe3f37300baa","9cf3f3ea0d6843adbcfd4795d1e4689a","3c7a82782ca64777a500c71e03347a1b","bc71209621cb44f1a50d2c2af5341a9b","260ea906c95247c0ad4fb2eea746f084","a9e85407f34b47d9b1da3b6e8e9d1385","90c700edbdd14a3ca62ef37eb3349f1c","99a8bd7f828249ed801f9dd21693dd94","b4a8fe80f97a4b3d98253bcb528002e4","c6ff221ebdd44d6481b234e867ee85f8","5d6c3acd3ac141bc9c768314407a9ec9","79aea2c3585143818469e9f21c30e660","d50f1828127d42f6a9ffd04fe1c6a230","4e467a18c3e648318b647b92da92c0b5","b3c46175c05d4a75856a49678be56195","f217d81f0429491aa7081cb371cf3da7","767fc6c00ed345199a1072e3b9b032cb","4f9f47ec76ac4513b655ccbd3f03bf02","25de3eb17d50427cb5b14a5a9d716cc9","ef3e32ac2c884aaaa69808cd4433849e","6cd515a4bc8240bb90c84d0cfa6c0b11","85c143ffb0ca458a815559f81c3b9d45","38b89735a6754179a32ee7f27abe0d25","b8