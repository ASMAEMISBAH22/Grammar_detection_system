generation_config.json:   0%|          | 0.00/142 [00:00<?, ?B/s]"],"application/vnd.jupyter.widget-view+json":{"version_major":2,"version_minor":0,"model_id":"4c0d20dc0a0049f1a362ba12bd751e24"}},"metadata":{}},{"output_type":"stream","name":"stdout","text":["Model loaded.\n"]}]},{"cell_type":"code","source":["def clean_prefix(text):\n","    # remove model returning \"gec :\" or similar\n","    if not text:\n","        return text\n","    # unify and strip several variants\n","    for prefix in [\"gec :\", \"Gec :\", \"gec:\", \"Gec:\", \"GEC :\", \"GEC:\"]:\n","        if text.startswith(prefix):\n","            return text[len(prefix):].strip()\n","    # some models return lowercase or spaced: safe lower-check\n","    ltext = text.lstrip()\n","    if ltext.lower().startswith(\"gec :\") or ltext.lower().startswith(\"gec:\"):\n","        # remove first token up to colon\n","        return ltext.split(\":\", 1)[1].strip()\n","    return text.strip()\n","\n","def batch_correct(sentences, batch_size=16, max_length=128, num_beams=1):\n","    \"\"\"Return list of corrected sentences for given list of input sentences.\"\"\"\n","    device_local = device\n","    outputs = []\n","    for i in tqdm(range(0, len(sentences), batch_size)):\n","        batch = sentences[i:i+batch_size]\n","        prefixed = [\"gec: \" + s for s in batch]\n","        enc = tokenizer(prefixed, padding=True, truncation=True, return_tensors=\"pt\").to(device_local)\n","        with torch.no_grad():\n","            preds = model.generate(\n","                **enc,\n","                max_length=max_length,\n","                num_beams=num_beams,\n","                do_sample=False\n","            )\n","        decoded = tokenizer.batch_decode(preds, skip_special_tokens=True)\n","        decoded = [clean_prefix(x) for x in decoded]\n","        outputs.extend(decoded)\n","    return outputs\n"],"metadata":{"id":"OwF43uXMdCAq","executionInfo":{"status":"ok","timestamp":1765385059798,"user_tz":-60,"el